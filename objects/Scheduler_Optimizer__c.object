<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Use these custom setting to run the Scheduler in minutes</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>On_Off_Switch__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Switch for scheduler weather to run with optimize code</description>
        <externalId>false</externalId>
        <label>On/Off Switch</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Scheduler_Job_Name__c</fullName>
        <description>Scheduler Job Name by which you want it to schedule.</description>
        <externalId>false</externalId>
        <label>Scheduler Job Name</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Time_in_Minutes_0_59__c</fullName>
        <description>Time in Minutes to schedule the job.</description>
        <externalId>false</externalId>
        <label>Time in Minutes(0-59)</label>
        <precision>2</precision>
        <required>true</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <label>Scheduler Optimizer</label>
    <visibility>Public</visibility>
</CustomObject>
